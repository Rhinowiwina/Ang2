<!DOCTYPE html>
<html >
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="cache-control" content="max-age=0" />
    <meta http-equiv="cache-control" content="no-cache" />
    <meta http-equiv="expires" content="0" />
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT" />
    <meta http-equiv="pragma" content="no-cache" />

    <title>Lifeline Services CA</title>

    <link rel="stylesheet" href="~/Content/js/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/sass/siteAngular.css">
    <link rel="stylesheet" href="https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0-rc6/angular-material.min.css">
    @Styles.Render("~/Content/razorCss")
    <style>
        .navbar { background-color:#A81217 !important}
    </style>
</head>
<body ng-app="app">
    <header id="header">
        <div>
            <nav>
                <div class="row header-banner header">
                    <div class="col-sm-2">
                        <a href="/" class="header-logo">
                            <img src="/Content/img/Arrow-Sales-Group-Logo.png">
                        </a>
                    </div>
                </div>

                <div class="header-menu navbar">

                </div>
            </nav>
        </div>
        <div ng-controller="NotificationCtrl">
            <div ng-repeat="message in messages">
                <div class="{{message.notificationClass}}" role="alert" ng-if="message.displayError">
                    <button class="close" aria-label="Close" ng-click="dismissAlert($index)"><span aria-hidden="true">&times;</span></button>
                    <span ng-bind-html="message.userMessage"></span>
                </div>
            </div>
        </div>
    </header>


    <div class="container container-content panel panel-default">
        @RenderBody()
    </div>

    <script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.1/js/bootstrap.min.js"></script>

    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.11/angular.min.js"></script>
    <script src="~/Content/js/angular-sanitize/angular-sanitize.min.js"></script>
    <script src="~/Content/js/angular-bootstrap/ui-bootstrap-tpls.min.js"></script>
    <script src="~/Content/js/angular-bootstrap-show-errors/src/showErrors.min.js"></script>
    <script src="~/Content/js/angular-sanitize/angular-sanitize.min.js"></script>
    <script src="~/Content/js/angular-loader/angular-loader.min.js"></script>
    <script src="~/Content/js/angular-animate/angular-animate.min.js"></script>
    <script src="~/Content/js/angular-cookies/angular-cookies.min.js"></script>
    @*<script src="~/Content/js/angular-file-upload.12.0.4/ng-file-upload.js"></script>*@
    <script src="~/Content/js/angular-file-upload.12.0.4/ng-file-upload.min.js"></script>
    @*<script src="~/Content/js/ng-file-upload/ng-file-upload.min.js"></script>*@
    <script src="~/Content/js/vendor/json2.min.js"></script>
    <script src="~/Content/js/vendor/ScriptCam/swfobject.js"></script>
    <script src="~/Content/js/vendor/ScriptCam/scriptcam.min.js"></script>

    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.9/angular-aria.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/angular_material/1.0.0-rc6/angular-material.min.js"></script>

    <script src="/v1.100000000000/app/usersignupsite.js"></script>

</body>
</html>